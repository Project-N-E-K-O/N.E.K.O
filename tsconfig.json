{
  "compilerOptions": {
    "target": "es2021",
    "module": "Node16",
    "lib": [
      "dom",
      "es2021"
    ],
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "jsx": "react-native",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
