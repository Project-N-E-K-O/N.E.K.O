    /**
     * æ£€æŸ¥å…ƒç´ æ˜¯å¦å¯è§
     */
    isElementVisible(element) {
        if (!element) return false;

        // æ£€æŸ¥ display å±žæ€§
        const style = window.getComputedStyle(element);
        if (style.display === 'none') {
            return false;
        }

        // æ£€æŸ¥ visibility å±žæ€§
        if (style.visibility === 'hidden') {
            return false;
        }

        // æ£€æŸ¥ opacity å±žæ€§
        if (style.opacity === '0') {
            return false;
        }

        // æ£€æŸ¥å…ƒç´ æ˜¯å¦åœ¨è§†å£å†…æˆ–è‡³å°‘æœ‰å°ºå¯¸
        const rect = element.getBoundingClientRect();
        if (rect.width === 0 || rect.height === 0) {
            return false;
        }

        return true;
    }

    /**
     * æ˜¾ç¤ºéšè—çš„å…ƒç´ ï¼ˆç”¨äºŽå¼•å¯¼ï¼‰
     */
    showElementForTutorial(element, selector) {
        if (!element) return;

        const style = window.getComputedStyle(element);

        // ä¿å­˜åŽŸå§‹æ ·å¼ï¼Œä»¥ä¾¿åŽç»­æ¢å¤
        const originalDisplay = element.style.display;
        const originalVisibility = element.style.visibility;
        const originalOpacity = element.style.opacity;

        // æ˜¾ç¤ºå…ƒç´ ï¼ˆä½¿ç”¨ !important ç¡®ä¿æ ·å¼è¢«åº”ç”¨ï¼‰
        if (style.display === 'none') {
            element.style.setProperty('display', 'flex', 'important');
            console.log(`[Tutorial] æ˜¾ç¤ºéšè—å…ƒç´ : ${selector}`);
