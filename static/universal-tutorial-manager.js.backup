        let attempts = 0;
        const maxAttempts = 100;

        const checkDriver = () => {
            attempts++;

            if (typeof window.driver !== 'undefined') {
                console.log('[Tutorial] driver.js å·²åŠ è½½');
                this.initDriver();
                return;
            }

            if (attempts >= maxAttempts) {
                console.error('[Tutorial] driver.js åŠ è½½å¤±è´¥ï¼ˆè¶…æ—¶ 10 ç§’ï¼‰');
                return;
            }

            setTimeout(checkDriver, 100);
        };

        checkDriver();
    }

